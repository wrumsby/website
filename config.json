{
  "locals": {
    "url": "http://localhost:8080",
    "name": "Walter Rumsby's Web Site",
    "owner": "Walter Rumsby"
  },
  "plugins": [
    "./plugins/paginator.coffee"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 3
  },
  "filenameTemplate": "/blog/posts/:year/:month/:day/:title/index.html",
  "output": "./blog",
  "port": 1337
}
